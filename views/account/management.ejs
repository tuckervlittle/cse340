<!-- Heading -->
<% if (title) { %>
  <h1>
    <%= title %>
  </h1>
  <% } else { res.redirect('/') }%>
    <%- messages() %>

      <% if (errors) { %>
        <ul class="error">
          <% errors.array().forEach(error=> { %>
            <% if (error.msg !='Invalid value' ) { %>

              <li>
                <%= error.msg %>
              </li>
              <% } %>
                <% }) %>
        </ul>
        <% }%>
        